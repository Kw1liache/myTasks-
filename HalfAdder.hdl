CHIP HalfAdder {
    IN a, b;    // 1-bit inputs
    OUT sum,    // Right bit of a + b 
        carry;  // Left bit of a + b

    PARTS:
    Nand(a=a, b=b, out=nand1);
    Nand(a=a, b=nand1, out=nand2);
    Nand(a=nand1, b=b, out=nand3);
	Nand(a=nand2, b=nand3, out=sum);
    Nand(a=nand1, b=nand1, out=carry);	
}
